        null

    cleanup : =>

        for fn in @_toClean
            @[fn] = null
            delete @[fn]

        null


    update: =>
        requestAnimFrame @update

        @preloaderView.update()
        @appView?.update()

        if @appView?
            @preloaderView.update( @appView.wrapper.home.interactive.helpButton )

